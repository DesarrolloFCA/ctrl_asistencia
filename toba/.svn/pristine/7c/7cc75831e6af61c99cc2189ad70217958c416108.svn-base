class AraiPackageTest extends PHPUnit_Framework_TestCase
{
    public function testGetPreSyncScript()
    {
        $nombreClase = "\\nombre\\clase";
        $p = new \SIU\AraiJsonParser\AraiPackage('nombre', 'desc', array('hooks'=>"$nombreClase"));
        $this->assertEquals($nombreClase, $p->getHooksScript());
    }
}
